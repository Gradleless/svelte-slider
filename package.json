{
  "name": "svelte-slider",
  "version": "0.0.1",
  "author": "Gradleless <https://github.com/Gradleless>",
	"description": "A svelte 5 slider customisable and lightweight",
	"scripts": {
		"build": "npm run package",
		"dev": "npm run watch",
		"dev:svelte": "vite dev",
		"package": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"watch": "svelte-kit sync && svelte-package --watch"
	},
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "publint": "^0.2.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.0.0",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "lucide-svelte": "^0.469.0"
  }
}
